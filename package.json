{
  "name": "profile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "rm -fr .next",
    "dev": "next dev",
    "gen-latex": "node scripts/gen-latex.js",
    "build:pdf": "pdflatex -interaction=nonstopmode cv.tex && mv cv.pdf public/ && rm -f cv.aux cv.log cv.out cv.synctex.gz",
    "prebuild": "npm run gen-latex && npm run build:pdf",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,json,css,scss}'"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^5.16.7",
    "handlebars": "^4.7.8",
    "i18next": "^23.14.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "next": "14.2.6",
    "react": "^18",
    "react-dom": "^18",
    "react-i18next": "^15.0.1",
    "react-intersection-observer": "^9.13.0",
    "react-tsparticles": "^2.12.2",
    "tsparticles-slim": "^2.12.0"
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^4.1.3",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "stylelint": "^16.8.2",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5"
  }
}
